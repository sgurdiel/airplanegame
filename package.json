{
  "name": "airplanegame_app",
  "version": "2.1.1",
  "description": "Airplane Game",
  "type": "module",
  "scripts": {
    "lint": "eslint src/**",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "dev:build": "webpack --config webpack.config.cjs --mode=development",
    "dev:watch": "webpack --config webpack.config.cjs --mode=development --watch",
    "test": "jest --config jest.config.cjs --verbose",
    "test:watch": "jest --config jest.config.cjs --watchAll",
    "pro:build": "webpack --config webpack.config.cjs --mode=production"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@eslint/js": "^9.37.0",
    "@jest/globals": "^29.3.1",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^9.37.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-loader": "^9.4.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^6.0.1"
  }
}
