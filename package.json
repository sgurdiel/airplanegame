{
  "name": "airplanegame_app",
  "version": "2.1.1",
  "description": "Airplane Game",
  "type": "module",
  "scripts": {
    "lint": "eslint src/**",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "dev:build": "webpack --config webpack.config.cjs --mode=development",
    "dev:watch": "webpack --config webpack.config.cjs --mode=development --watch",
    "test": "jest --verbose",
    "test:watch": "jest --watchAll",
    "pro:build": "webpack --config webpack.config.cjs --mode=production"
  },
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@jest/globals": "^30.2.0",
    "eslint": "^9.37.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^6.0.1"
  }
}
